<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Control colors.-->
    <Color x:Key="DisabledControlDarkColor">#FFC5CBF9</Color>
    <Color x:Key="DisabledForegroundColor1">#FF888888</Color>
    <Color x:Key="ControlLightColor">White</Color>
    <Color x:Key="ElectricBlue">#FF65C3F7</Color>

    <!--Border colors-->
    <Color x:Key="BorderMediumColor">#FF888888</Color>
    <Color x:Key="DisabledControlLightColor">#FFE8EDF9</Color>

    <Color x:Key="CMSBackgroundColor">#FF2B2B2B</Color>
    <Color x:Key="CMSInsetColor">#FF202020</Color>
    <Color x:Key="CMSSolidBorderColor">#FF484848</Color>
    <Color x:Key="CMSDarkBorderColor">#FF383838</Color>
    <Color x:Key="CMSLightBorderColor">#FF606060</Color>
    <Color x:Key="CMSCtrlButtonBackgroundColor">#FF4E4E4E</Color>
    <Color x:Key="CMSCtrlButtonBackgroundHoverColor">#FF606060</Color>
    <Color x:Key="CMSCtrlButtonForegroundColor">#FFA9A9A9</Color>
    <Color x:Key="CMSCtrlButtonForegroundHoverColor">#FFD8D8D8</Color>
    <Color x:Key="CMSCtrlButtonForegroundDisabledColor">#FF808080</Color>
    <Color x:Key="CMSCtrlButtonForegroundPressedColor">Yellow</Color>
    <Color x:Key="CMSCtrlButtonForegroundPressedColor1">#FFF8F8F8</Color>
    <Color x:Key="CMSCtrlLightBodyColor">#FF4A433B</Color>
    <Color x:Key="CMSCtrlLightHoverColor">#FF3A332B</Color>
    <Color x:Key="CMSSpinTextSelectionColor">#FFA0A0A0</Color>
    <Color x:Key="CMSSeparatorStartColor">#FF222222</Color>
    <Color x:Key="CMSSeparatorEndColor1">#FF666666</Color>
    <Color x:Key="CMSSeparatorEndColor">#FF444444</Color>
    <Color x:Key="CMSMenuBackgroundColor">#FF585858</Color>

    <Color x:Key="CtrlNormalStartColor">#FF585858</Color>
    <Color x:Key="CtrlNormalEndColor">#FF282828</Color>
    <Color x:Key="CtrlPressedStartColor">#FF505050</Color>
    <Color x:Key="CtrlPressedEndColor">#FF383838</Color>
    <Color x:Key="CtrlPressedBorderColor">#FF555555</Color>
    <Color x:Key="CtrlFocusedStartColor">#FF989898</Color>
    <Color x:Key="CtrlFocusedEndColor">#FF787878</Color>
    <Color x:Key="CtrlHoverStartColor">#FF686868</Color>
    <Color x:Key="CtrlCheckedStartColor">#FF404040</Color>
    <Color x:Key="CtrlCheckedEndColor">#FF181818</Color>
    <Color x:Key="CtrlCheckedBorderColor">#FF666666</Color>
    <Color x:Key="RaisedHighlightColor">#FF888888</Color>
    <Color x:Key="DisabledHighlightColor">#FF505050</Color>
    <Color x:Key="DisabledShadowColor">#FF303030</Color>
    <Color x:Key="ForegroundColor">#FFDDDDDD</Color>
    <Color x:Key="RaisedShadowColor">#FF000000</Color>
    <Color x:Key="DisabledForegroundColor">#FF777777</Color>
    <Color x:Key="CMSHeaderBackgroundColor">#FF2A709C</Color>
    <Color x:Key="CMSCellSelectionColor">#80FF0000</Color>
    <Color x:Key="CMSThumbnailSelectionColor">#FF2699D6</Color>

    <SolidColorBrush x:Key="ElectricBlueBrush" Color="{StaticResource ElectricBlue}"/>
    <SolidColorBrush x:Key="Highlight" Color="#808080" />
    <SolidColorBrush x:Key="Highlight1" Color="#606060" />
    <SolidColorBrush x:Key="Shadow" Color="#101010" />
    <SolidColorBrush x:Key="SunkenBackground" Color="#444444" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}" />
    <SolidColorBrush x:Key="RaisedHighlightBrush" Color="{StaticResource RaisedHighlightColor}" />
    <SolidColorBrush x:Key="RaisedShadowBrush" Color="{StaticResource RaisedShadowColor}" />
    <SolidColorBrush x:Key="CtrlBorderBrush" Color="{StaticResource CtrlNormalEndColor}" />
    <SolidColorBrush x:Key="PopupBorderBrush" Color="{StaticResource CtrlNormalStartColor}" />
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="{StaticResource DisabledForegroundColor}" />

    <SolidColorBrush x:Key="CMSSeparatorStartBrush" Color="{StaticResource CMSSeparatorStartColor}" />
    <SolidColorBrush x:Key="CMSSeparatorEndBrush" Color="{StaticResource CMSSeparatorEndColor}" />
    <SolidColorBrush x:Key="CMSSpinTextSelectionBrush" Color="{StaticResource CMSSpinTextSelectionColor}"/>
    <SolidColorBrush x:Key="CMSCtrlButtonBackgroundBrush" Color="{StaticResource CMSCtrlButtonBackgroundColor}"/>
    <SolidColorBrush x:Key="CMSCtrlButtonBackgroundHoverBrush" Color="{StaticResource CMSCtrlButtonBackgroundHoverColor}"/>
    <SolidColorBrush x:Key="CMSCtrlButtonForegroundBrush" Color="{StaticResource CMSCtrlButtonForegroundColor}"/>
    <SolidColorBrush x:Key="CMSCtrlButtonForegroundHoverBrush" Color="{StaticResource CMSCtrlButtonForegroundHoverColor}"/>
    <SolidColorBrush x:Key="CMSCtrlButtonForegroundDisabledBrush" Color="{StaticResource CMSCtrlButtonForegroundDisabledColor}"/>
    <SolidColorBrush x:Key="CMSLabelForegroundBrush" Color="{StaticResource CMSCtrlButtonForegroundColor}"/>

    <SolidColorBrush x:Key="CMSCtrlLightBodyBrush" Color="{StaticResource CMSCtrlLightBodyColor}"/>
    <SolidColorBrush x:Key="CMSCtrlInsetBrush" Color="{StaticResource CMSInsetColor}"/>
    <SolidColorBrush x:Key="CMSSolidBackgroundBrush" Color="{StaticResource CMSBackgroundColor}"/>
    <SolidColorBrush x:Key="CMSBackgroundBrush" Color="{StaticResource CMSBackgroundColor}"/>
    <SolidColorBrush x:Key="CMSSolidBorderBrush" Color="{StaticResource CMSSolidBorderColor}"/>
    <SolidColorBrush x:Key="CMSDarkBorderBrush" Color="{StaticResource CMSDarkBorderColor}"/>
    <SolidColorBrush x:Key="CMSLightBorderBrush" Color="{StaticResource CMSLightBorderColor}"/>
    <SolidColorBrush x:Key="CMSCtrlPressedBorderBrush" Color="{StaticResource CtrlPressedBorderColor}"/>
    <SolidColorBrush x:Key="CMSCtrlCheckedBorderBrush" Color="{StaticResource CtrlCheckedBorderColor}"/>
    <SolidColorBrush x:Key="CMSInsetPanelBrush" Color="{StaticResource CMSDarkBorderColor}"/>
    <SolidColorBrush x:Key="CMSHeaderBackgroundBrush" Color="{StaticResource CMSHeaderBackgroundColor}"/>
    <SolidColorBrush x:Key="CMSCellSelectionBrush" Color="{StaticResource CMSCellSelectionColor}"/>
    <SolidColorBrush x:Key="CMSThumbnailSelectionBrush" Color="{StaticResource CMSThumbnailSelectionColor}"/>
    <SolidColorBrush x:Key="CMSMenuBackgroundBrush" Color="{StaticResource CMSMenuBackgroundColor}"/>
    <SolidColorBrush x:Key="ToolbarBackgroundBrush" Color="#FF343434"/>

    <LinearGradientBrush x:Key="CMSHeaderBackgroundBrush1" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF343434" Offset="0.059"/>
        <GradientStop Color="#FF1B1B1B" Offset="0.982"/>
        <GradientStop Color="#FF171717" Offset="0.695"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CtrlNormalBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="{StaticResource CtrlNormalStartColor}" />
        <GradientStop Offset="1.2" Color="{StaticResource CtrlNormalEndColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CtrlHighlightBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="{StaticResource DisabledHighlightColor}" />
        <GradientStop Offset="1.2" Color="{StaticResource DisabledShadowColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CMSCtrlBodyBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF48413B" Offset="0.217"/>
        <GradientStop Color="#FF302D28" Offset="0.957"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CMSCtrlBorderBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF665F59" Offset="0.019"/>
        <GradientStop Color="#FF5B524B" Offset="0.028"/>
        <GradientStop Color="#FF383431" Offset="0.962"/>
        <GradientStop Color="#FF5B544E" Offset="0.972"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CMSCtrlDisabledBodyBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#80888888" Offset="0.217"/>
        <GradientStop Color="#80585858" Offset="0.957"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CMSStatusBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF363636" Offset="0.07"/>
        <GradientStop Color="#FF292929" Offset="0.965"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CMSHeaderBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource CMSCtrlButtonBackgroundColor}" Offset="0.045"/>
        <GradientStop Color="#FF2A2A2A" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="CMSCheckedBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0.0" Color="{StaticResource CtrlCheckedStartColor}" />
        <GradientStop Offset="1.2" Color="{StaticResource CtrlCheckedEndColor}" />
    </LinearGradientBrush>

    <LinearGradientBrush
        x:Key="CMSHatchBrushRed"
        StartPoint="0,0"
        EndPoint="5,5"
        SpreadMethod="Repeat"
        MappingMode="Absolute">
        <GradientStop Color="#80FF0000" Offset="0.0"/>
        <GradientStop Color="#80FF0000" Offset="0.1"/>
        <GradientStop Color="Transparent" Offset="0.1"/>
        <GradientStop Color="Transparent" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush
        x:Key="CMSHatchBrushBlueFilled"
        StartPoint="0,0"
        EndPoint="4,4"
        SpreadMethod="Repeat"
        MappingMode="Absolute">
        <GradientStop Color="#FF2699D6" Offset="0.0"/>
        <GradientStop Color="#FF2699D6" Offset="0.1"/>
        <GradientStop Color="#FF383838" Offset="0.1"/>
        <GradientStop Color="#FF383838" Offset="1.0"/>
    </LinearGradientBrush>

    <LinearGradientBrush
        x:Key="CMSHatchBrushGray"
        StartPoint="0,0"
        EndPoint="-3,3"
        SpreadMethod="Repeat"
        MappingMode="Absolute">
        <GradientStop Color="#FF808080" Offset="0.0"/>
        <GradientStop Color="#FF808080" Offset="0.1"/>
        <GradientStop Color="White" Offset="0.1"/>
        <GradientStop Color="White" Offset="1.0"/>
    </LinearGradientBrush>
</ResourceDictionary>
